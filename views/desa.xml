<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="cdn_desa_list" model="ir.ui.view">
            <field name="name">cdn.desa.form</field>

            <field name="model">cdn.desa</field>

            <field name="arch" type="xml">
                <list>
                    <field name="name" />

                    <field name="kode" />

                    <field name="singkatan" />

                    <field name="description" />

                    <field name="kecamatan_id" />
                </list>
            </field>
        </record>

        <record id="cdn_desa_form" model="ir.ui.view">
            <field name="name">cdn.desa.form</field>

            <field name="model">cdn.desa</field>

            <field name="arch" type="xml">
                <form string="Desa">
                    <sheet>
                        <group>
                            <field name="name" />

                            <field name="kode" />

                            <field name="singkatan" />

                            <field name="description" />

                            <field name="kecamatan_id" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="cdn_desa_action" model="ir.actions.act_window">
            <field name="name">Desa</field>

            <field name="type">ir.actions.act_window</field>

            <field name="res_model">cdn.desa</field>

            <field name="view_mode">list,form</field>
        </record>

        <menuitem id="desa_menu_act" name="Desa" parent="wilayah_menu_categ" action="cdn_desa_action" sequence="40" />
    </data>
</odoo>